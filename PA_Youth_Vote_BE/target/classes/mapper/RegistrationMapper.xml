<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace:改mapper.xml映射文件的唯一标识并且必须和数据处理层的接口的路径相同-->
<mapper namespace="com.votez.mapper.RegistrationMapper">
    <insert id="register" parameterType="com.votez.model.Registration" >
        insert into registration value (#{email},#{zipCode},#{name},#{firstName},#{lastName},
        #{address},#{city},#{state},#{birthDay},#{phone})
    </insert>
</mapper>